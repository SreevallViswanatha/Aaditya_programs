#include <stdio.h>
#include <stdlib.h>

int main()
{

   int firstNumber;
   int secondNumber;
   int product;
   printf("\nEnter the first number: ");
   scanf("%d", &firstNumber);
   printf("\nEnter second number: ");
   scanf("%d", &secondNumber);
   product=firstNumber*secondNumber;
   printf("\n\nThe product is: %d\n\n\n", product);
}
