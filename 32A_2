#include <stdio.h>
#include <stdlib.h>

int main()
{

   int firstNumber;
   int secondNumber;
   int sum;
   printf("\nEnter the first number: ");
   scanf("%d", &firstNumber);
   printf("\nEnter the second number: ");
   scanf("%d", &secondNumber);
   sum=(firstNumber+secondNumber);
   printf("\n\nThe sum is: %d\n\n\n", sum);
}
