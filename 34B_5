#include <stdio.h>
#include <stdlib.h>

int main()
{
    int number, sum, remainder, num;
    sum=0;
    printf("Enter a five digit number: ");
    scanf("%d", &number);
    num=number;
    remainder=number%10;
    sum=sum+remainder;
    number=number/10;
    remainder=number%10;
    sum=sum+remainder;
    number=number/10;
    remainder=number%10;
    sum=sum+remainder;
    number=number/10;
    remainder=number%10;
    sum=sum+remainder;
    number=number/10;
    sum=sum+number;
    printf("The sum of the five digit number %d you have entered is: %d", num, sum);
}
