#include <stdio.h>
#include <stdlib.h>

int main()
{

    float selling_price, profit, cost_price;
    printf("Enter  the selling price: ");
    scanf("%f", &selling_price);
    printf("Enter  the profit: ");
    scanf("%f", &profit);
    cost_price=(selling_price-profit)/15;
    printf("The cost price is: %f", cost_price);
}
