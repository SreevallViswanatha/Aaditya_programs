#include <stdio.h>
#include <stdlib.h>

int main()
{

   float selling_price, profit, cost_price;
   printf("Enter the selling price of 15 items: ");
   scanf("%f", &selling_price);
   printf("Enter the profit of 15 items: ");
   scanf("%f", &profit);
   cost_price=(selling_price-profit)/15;
   printf("The cost price of of one item is: %.0f", cost_price);
}
